<h1>Sign up</h1>

<%= form_for(@member) do |f| %>
	<%= render 'shared/error_messages' %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :password %>
  <%= f.password_field :password %>
  
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Create New Member" %>

<% end %>
